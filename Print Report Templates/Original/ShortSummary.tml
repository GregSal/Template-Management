LineSpacing=1.3
Margin=100
# 10 mm left margin in the form.

Move=0
Font=84;900;Arial
Text=TREATMENT PLAN REPORT
RelativeMove=0;-10
HorizontalLine=0
RelativeMove=0;2
HorizontalLine=0
RelativeMove=0;10

##### PATIENT DATA
Move=0
Font=42;700;Arial
Text=
Text=Patient

Margin=150
# Line 1: patient name and IDs
Move=0
Font=30;700;Arial
LineText=Name:
Move=300
Font=30;400;Arial
LineText=$PatientFirstName  $PatientMiddleName  $PatientLastName
Move=900
Font=30;700;Arial
LineText=IDs:
Font=30;400;Arial
Move=1200
Text=$PatientId , $PatientId2

# Line 2: Birth Date and sex
Move=0
Font=30;700;Arial
LineText=Birthdate:
Move=300
Font=30;400;Arial
LineText=$PatientDateOfBirth
Move=900
Font=30;700;Arial
LineText=Sex:
Move=1200
Font=30;400;Arial
Text=$PatientSex

# Line3: Primary Oncologist
Move=0
Font=30;700;Arial
LineText=Primary Oncologist:
Move=300
Font=30;400;Arial
Text=$PrimaryOncologist

# Lines 4-: comments
Move=0
Font=30;700;Arial
LineText=Report Comment:
Move=300
Font=30;400;Arial
If=$Comment
  Text=$Comment
Else
  Text=
EndIf

If=$PatientComment
  Move=0
  Font=30;700;Arial
  LineText=Patient Comment:
  Move=300
  Font=30;400;Arial
  Text=$PatientComment
EndIf
RelativeMoveText=0;1

##### COURSE & PLAN DATA
Margin=100
Move=0
Font=42;700;Arial
Text=Plan
Margin=150

# Line 1: Plan ID and name
Move=0
Font=30;700;Arial
LineText=Plan ID:
Move=300
Font=30;400;Arial
LineText=$PlanId
Move=900
Font=30;700;Arial
LineText=Plan Name:
Move=1200
Font=30;400;Arial
Text=$PlanName

# Line 1b: Plan comment, if defined
If=$PlanComment
  Move=0
  Font=30;700;Arial
  LineText=Plan Comment:
  Move=300
  Font=30;400;Arial
  Text=$PlanComment
EndIf

# Line 2: Course ID and intent
Move=0
Font=30;700;Arial
LineText=Course ID:
Move=300
Font=30;400;Arial
LineText=$CourseId
Move=900
Font=30;700;Arial
LineText=Course Intent:
Move=1200
Font=30;400;Arial
Text=$CourseIntent

# Line 2b: Course comment, if defined
If=$CourseComment
  Move=0
  Font=30;700;Arial
  LineText=Course Comment:
  Move=300
  Font=30;400;Arial
  Text=$CourseComment
EndIf

# Line 3: Image ID and name
If=$IsIRREGPlan
Else
  Move=0
  Font=30;700;Arial
  LineText=Image ID:
  Move=300
  Font=30;400;Arial
  LineText=$ImageId
  Move=900
  Font=30;700;Arial
  LineText=Image Name:
  Move=1200
  Font=30;400;Arial
  Text=$ImageName
EndIf

# Line 3b: Image comment, if defined
If=$ImageComment
  Move=0
  Font=30;700;Arial
  LineText=Image Comment:
  Move=300
  Font=30;400;Arial
  Text=$ImageComment
EndIf

Move=0
Font=30;700;Arial
LineText=Treatment Orientation:
Move=300
Font=30;400;Arial
LineText=$PatientTreatmentOrientation

If=$IsIRREGPlan
  Text=
Else
  Move=900
  Font=30;700;Arial
  LineText=Image Origin:
  Move=1200
  Font=30;400;Arial
  Text=$ImageUserOrigin
EndIf

RelativeMoveText=0;1

##### PRESCRIPTION & FRACTIONATION DATA
Margin=100
Move=0
Font=42;700;Arial
Text=Dose Prescription

Margin=150
Move=0

Font=30;700;Arial
LineText=Target Volume:
Move=500
Font=30;400;Arial
LineText=$TargetVolume

Move=900
Font=30;700;Arial
LineText=Primary Reference Point:
Move=1300
Font=30;400;Arial

If=$IsIRREGPlan
  Text=$PrimaryRefPointId
Else
  Text=$PrimaryRefPointId  ($RelativeDoseInPrimaryRefPoint )

  Move=0
  Font=30;700;Arial
  LineText=Prescribed Dose Percentage:
  Move=500
  Font=30;400;Arial
  LineText=$PrescribedDosePerCent

  Move=900
  Font=30;700;Arial
  LineText=Plan Normalization Value:
  Move=1300
  Font=30;400;Arial
  Text=$PlanNormValue
EndIf


Move=0
Loop=Fractionations
  Move=0
  Font=30;700;Arial
  LineText=Fractionation: 
  Move=500
  Text=$FractionationId

  If=$IsIRREGPlan
  Else
    Move=50
    Font=30;400;Arial
    LineText=Prescribed Dose:
    Move=500
    Text=$PrescribedDose  ($PrescribedDosePerFraction  / fraction)
  EndIf
  Move=50
  Font=30;400;Arial
  LineText=Dose at primary reference point:
  Move=500
  Text=$DoseInPrimaryRefPoint  ($DosePerFractionInPrimaryRefPoint  / fraction)
  Move=50
  LineText=Number of Fractions:
  Move=500
  Text=$Fractions 
EndLoop
RelativeMoveText=0;1

PageBreak=0.9
##### FIELD DATA
Margin=100
Move=0
Font=42;700;Arial
Text=Fields
Margin=150

Loop=Fractionations
  Move=0
  Font=30;700;Arial
  Text=$FractionationId
  Font=24;600;Arial
  Move=0
  LineText=Id
#  Move=100
#  LineText=Technique
  Move=150
  LineText=Machine
  Move=300
  LineText=Energy
  Move=400
  LineText=Scale
  Move=650
  LineTextRight=Weight
  Move=850
  LineTextRight=Size
  Move=1100
  LineTextRight=Gantry
  Move=1200
  LineTextRight=Collimator
  Move=1300
  LineTextRight=Table
  Move=1400
  LineTextRight=X
  Move=1500
  LineTextRight=Y
  Move=1600
  LineTextRight=Z
  Move=1700
  LineTextRight=SSD
  Move=2100
  LineTextRight=MU
  Move=2150
  Text=Wedge

  RelativeMove=0;1
  HorizontalLine=0
  RelativeMove=0;1

  Loop=Fields
    Font=24;400;Arial
    Move=0
    LineText=$FieldId
#    Move=100
#    LineText=$FieldTechnique  $IsFieldFixedSSD
    Move=150
    LineText=$FieldMachineId
    Move=300
    LineText=$FieldEnergyMode
    Move=400
    LineText=$FieldMachineScale
    Move=650
    LineTextRight=$FieldWeightFactor
    Move=850
    LineTextRight=$FieldSizeX  x $FieldSizeY
    If=$IsFieldSymmetric
	    LineText=
    Else
	    LineText=(Asym)
    EndIf

    Move=1100
    If=$FieldGantryRotationDir
      LineTextRight=$FieldGantryAngle  - $FieldGantryStopAngle  
    Else
      LineTextRight=$FieldGantryAngle
    EndIf
    Move=1200
    LineTextRight=$FieldCollimatorAngle
    Move=1300
    LineTextRight=$FieldTableAngle
    Move=1400
    LineTextRight=$FieldIsocentreX
    Move=1500
    LineTextRight=$FieldIsocentreY
    Move=1600
    LineTextRight=$FieldIsocentreZ
    Move=1700
    LineTextRight=$FieldSSD
    Move=2100
    LineTextRight=$FieldMonitorUnits
    Move=2150
    Loop=Wedges
      LineText=$WedgeId
      RelativeMoveText=1;0  
    EndLoop
    Text=
  EndLoop
EndLoop
RelativeMoveText=0;1

PageBreak=0.9
Move=0
Font=30;700;Arial
Text=Calculation Errors And Warnings:

Loop=Fields
    Font=30;400;Arial
    Loop=CalculationErrors
      Move=50
      If=$FieldCalculationError
        Text=$FieldCalculationError  (Error: $FieldId )
      EndIf
    EndLoop
    Loop=CalculationWarnings
      Move=50
      If=$FieldCalculationWarning
        Text=$FieldCalculationWarning  (Warning: $FieldId )
      EndIf
    EndLoop
EndLoop
