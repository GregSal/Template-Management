			Move=300
			Font=40;400;Arial
			Text=$FieldIsocentreX , $FieldIsocentreY , $FieldIsocentreZ
			Move=0
			Move=100
			Font=40;700;Arial
			LineText=Source Skin Distance = 
			Move=500
			Font=40;400;Arial
			LineText=$FieldSSD
			Move= 650
			  If=$FieldBoluses
			  Font=40;700;Arial
			  LineText=Source to Bolus Distance = 
			  Move=1100
			  Loop=Boluses			  
			  Font=40;400;Arial
			  Text=$FieldSBD
			  EndLoop
			 Else
			  Font=40;700;Arial
			  Text = NO BOLUS
			 EndIf
			HorizontalLine=0;1800
			RelativeMoveText=0;0.5
			
		EndIf
	EndLoop
	#End of Fraction Loop
  
  EndIf
  #End of IsSetupField Else
	
EndLoop
