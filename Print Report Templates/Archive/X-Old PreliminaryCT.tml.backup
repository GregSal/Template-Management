Section=Form
LineSpacing=1.3
Margin=200
# 2 cm left margin in the form.

Move=0
Font=70;900;Arial
Text=PRELIMINARY CT PLAN REPORT
RelativeMove=0;-10
HorizontalLine=0;1750
RelativeMove=0;1
HorizontalLine=0;1750
RelativeMove=0;5

##### PATIENT DATA
Move=0
Font=50;700;Arial
Text=
Text=PATIENT

Margin=250
# Line 1: patient name and IDs
Move=0
Font=30;700;Arial
LineText=Name:
Move=300
Font=30;400;Arial
LineText=$PatientLastName , $PatientFirstName  $PatientMiddleName  
Move=900
Font=30;700;Arial
LineText=IDs:
Font=30;400;Arial
Move=1200
LineText=$PatientId
If=$PatientId2
  Text=, $PatientId2
Else
  Text=
EndIf

# Line 2: Birth Date and sex
Move=0
Font=30;700;Arial
LineText=Birthdate:
Move=300
Font=30;400;Arial
LineText=$PatientDateOfBirth
Move=900
Font=30;700;Arial
LineText=Sex:
Move=1200
Font=30;400;Arial
Text=$PatientSex

# Line3: Primary Oncologist
Move=0
Font=30;700;Arial
LineText=Primary Oncologist:
Move=300
Font=30;400;Arial
Text=$PrimaryOncologist

# Lines 4-: comments
Move=0
Font=30;700;Arial
LineText=Report Comment:
Move=300
Font=30;400;Arial
If=$Comment
  Text=$Comment
Else
  Text=
EndIf

If=$PatientComment
  Move=0
  Font=30;700;Arial
  LineText=Patient Comment:
  Move=300
  Font=30;400;Arial
  Text=$PatientComment
EndIf
RelativeMoveText=0;1

PageBreak=0.9

##### PROTOCOL DATA

If=$ProtocolId
  Margin=200
  Move=0
  Font=60;700;Arial
  Text=PROTOCOL

  Margin=250
  Move=0
  Font=30;700;Arial
  LineText=Protocol Id:
  Move=300
  Font=30;400;Arial
  Text=$ProtocolId

  Move=0
  Font=30;700;Arial
  Font=30;400;Arial
  LineText=Status:
  Move=300
  Text=$ProtocolStatus

  # Phases
  Loop=Phases

    If=$IsPlanPhase
      # Phase information
      Move=0
      Font=30;700;Arial
      LineText=Protocol Plan Id:
      Move=300
      Font=30;400;Arial
      Text=$PhaseId

      Move=0
      Font=30;700;Arial
      LineText=Total Fractions:
      Move=300
      Font=30;400;Arial
      Text=$PhaseTotalFractions

      Move=0
      Font=30;700;Arial
      LineText=Fractions Per Week:
      Move=300
      Font=30;400;Arial
      Text=$PhaseFractionsPerWeek

      Move=0
      Font=30;700;Arial
      LineText=Fractions Per Day:
      Move=300
      Font=30;400;Arial
      Text=$PhaseFractionsPerDay
      RelativeMoveText=0;1  	
    
      # Phase prescriptions    
      Move=0
      Font=30;700;Arial
      Text=Prescriptions:

      # Header Line
      Move=30
      Font=30;400;Arial
      LineText=Prescription
      Move=1150
      Text=Dose
      Font=30;400;Arial
      HorizontalLine=30;1320

      # Prescription items
      Loop=PrescriptionItems
        Move=30
        LineText=$PrescriptionType
        Move=330
        LineText=$PrescriptionModifier
        Move=630
        LineText=$PrescriptionText
        Move=1150
        Text=$PrescriptionDose
      EndLoop
      RelativeMoveText=0;1 
    EndIf
  EndLoop
EndIf

#### PLAN & COURSE DATA

Margin=200
Move=0
Font=50;700;Arial
Text=PLAN

Font=42;700;Arial
Text=General
