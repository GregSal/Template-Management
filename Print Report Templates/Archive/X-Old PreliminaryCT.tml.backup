#### PATIENT ORIENTATION & IMAGE ORIGIN & PLANNING COORDINATE SYSTEM

If=$IsIRREGPlan
  Move=0
  Font=30;700;Arial
  LineText=Patient Treatment Orientation:
  Move=600
  Font=30;400;Arial
  Text=$PatientTreatmentOrientation
  RelativeMoveText=0;1
Else
  RelativeMoveText=0;1

  Margin=200

  Move=0
  Font=42;700;Arial
  Text=Image

  Margin=250

  # Image Id & name
  Move=0
  Font=30;700;Arial
  LineText=Image Id:
  Move=600
  Font=30;400;Arial
  Text=$ImageId
  Move=0
  Font=30;700;Arial
  LineText=Image Name:
  Move=600
  Font=30;400;Arial
  Text=$ImageName
  Move=0
  Font=30;700;Arial
  LineText=Image Series Id:
  Move=600
  Font=30;400;Arial
  Text=$ImageSeriesId
  Move=0
  Font=30;700;Arial
  LineText=Image Modality:
  Move=600
  Font=30;400;Arial
  Text=$ImageModality

  # Image comment, if defined
  If=$ImageComment
    Move=0
    Font=30;700;Arial
    LineText=Image Comment:
    Move=600
    Font=30;400;Arial
    Text=$ImageComment
  EndIf

  Move=0
  Font=30;700;Arial
  LineText=Patient Imaging Orientation:
  Move=600
  Font=30;400;Arial
  Text=$PatientImagingOrientation

  Move=0
  Font=30;700;Arial
  LineText=Patient Treatment Orientation:
  Move=600
  Font=30;400;Arial
  Text=$PatientTreatmentOrientation

  Move=0
  Font=30;700;Arial
  LineText=Planning Coordinate System:
  Move=600
  Font=30;400;Arial
  Text=$PlanningCoordinateSystemName

  Move=0
  Font=30;700;Arial
  LineText=Image Origin:
  Move=600
  Font=30;400;Arial
  Text=$ImageUserOrigin

  If=$ImageUserOriginComment
    Move=0
    Font=30;700;Arial
    LineText=Image Origin Comment:
    Move=600
    Font=30;400;Arial
    Text=$ImageUserOriginComment
  EndIf

  Move=0
  Font=30;700;Arial
  If=$ImageApprovalDate
    LineText=Image Approval:
    Move=600
    Font=30;400;Arial
    Text=$ImageApprovalDate  by $ImageApprover
  Else
    Text=IMAGE NOT APPROVED
  EndIf
EndIf

RelativeMoveText=0;1

PageBreak=0.9

##### WARNINGS
