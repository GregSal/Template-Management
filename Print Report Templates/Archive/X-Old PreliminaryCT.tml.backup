Move=900
Font=30;700;Arial
LineText=Course Intent:
Move=1200
Font=30;400;Arial
Text=$CourseIntent

If=$PlanComment
  Move=0
  Font=30;700;Arial
  LineText=Plan Comment:
  Move=300
  Font=30;400;Arial
  Text=$PlanComment
EndIf

If=$CourseComment
  Move=0
  Font=30;700;Arial
