Move=0
Font=94;900;Arial
Text=SUMPLAN REPORT
RelativeMove=0;-10
HorizontalLine=0;1750
RelativeMove=0;2
HorizontalLine=0;1750
RelativeMove=0;10
Text=

Font=34;400;Arial
LineText=Patient:
Move=250
Font=60;700;Arial
Text=$PatientLastName , $PatientFirstName  $PatientMiddleName 
Move=0
Font=34;400;Arial
LineText=Patient IDs:
Move=250
Font=60;700;Arial
LineText=$PatientId
If=$PatientId2
  Text=, $PatientId2
Else
  Text=
EndIf

Move=0
Font=34;400;Arial
LineText=Date:
Move=250
Font=60;700;Arial
Text=$CurrentDateShort

Move=0
Font=34;400;Arial
LineText=Plan Sum Id:
Font=60;700;Arial
Move=250
Text=$PlanSumId

Move=0
Font=34;400;Arial
LineText=Plan Sum Name:
Font=60;700;Arial
Move=250
Text=$PlanSumName

Move=0
Font=34;400;Arial
LineText=Course:
Font=60;700;Arial
Move=250
Text=$PlanSumCourseId

LineSpacing=1.1

Loop=SumPlans
  Font=34;400;Arial
  Text=
  Move=0
  LineText=Plan Id:
  Move=250
  Font=42;400;Arial
  Text=$PlanIdOfSum
  Font=34;400;Arial
  Move=0
  LineText=Plan Name:
  Move=250
  Font=42;400;Arial
  Text=$PlanNameOfSum
  Move=0
  Font=34;400;Arial
  LineText=Fractionation Id
  Move=400
  LineText=Prescription dose
  Move=1000
  LineText=Nbr of Fractions
  Move=1300 
  Text=Dose at primary reference point
  Font=42;400;Arial
  HorizontalLine=0;1750
  Loop=SumFractions
    Move=0
    LineText=$FractionationIdOfSum
    Move=400
    LineText=$TotalDose_ForSum ($DosePerFraction_ForSum  / fraction)
    Move=1000
    LineText=$NbrOfFrac_ForSum
    Move=1300
    Text=$TotalDoseAtPrimary_ForSum
  EndLoop
EndLoop


PageBreak=0
